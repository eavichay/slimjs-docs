{
  "mount": {
    "public": {
      "url": "/",
      "static": true,
      "resolve": false
    },
    "src": "/src"
  },
  "buildOptions": {
    "sourcemap": true
  },
  "alias": {},
  "workspaceRoot": "../../",
  "plugins": [
    [
      "snowpack-plugin-raw-file-loader",
      {
        "exts": [".hbs", ".md"]
      }
    ],
    [
      "@snowpack/plugin-babel",
      {
        "input": [".js"],
        "transformOptions": {
          "plugins": [
            ["@babel/plugin-proposal-decorators", { "legacy": true }],
            ["@babel/plugin-proposal-class-properties", { "loose": true }]
          ]
        }
      }
    ]
  ]
}
